{
    "name": "smogon-data",
    "version": "0.1.0",
    "description": "Website to display Smogon Usage Statistics for Pokemon Showdown",
    "main": "server.js",
    "scripts": {
        "buildDev": "webpack --mode development --config webpack.dev.config.js && webpack --mode development --config webpack.server.config.js",
        "buildProd": "webpack --mode production --config webpack.prod.server.config.js && webpack --mode production --config webpack.config.js",
        "start": "webpack-dev-server --mode development --config webpack.dev.config.js",
        "test": "jest",
        "test:watch": "npm test -- --watch"
    },
    "repository": "https://github.com/alyons/sweepercalc-2.0",
    "dependencies": {
        "express": "^4.17.1",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-router-dom": "^5.1.2"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!**/index.js",
            "!**/LogIn.js"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 70,
                "functions": 80,
                "lines": 80,
                "statements": 80
            }
        },
        "moduleNameMapper": {
            "\\.(ico|svg)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css)$": "<rootDir>/__mocks__/styleMock.js"
        }
    },
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "@babel/preset-env": "^7.6.2",
        "@babel/preset-react": "^7.0.0",
        "aws-sdk": "^2.536.0",
        "axios": "^0.19.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "bootstrap": "^4.3.1",
        "clean-webpack-plugin": "^2.0.2",
        "cookie-parser": "^1.4.4",
        "cookie-session": "^1.3.3",
        "css-loader": "^2.1.1",
        "dotenv": "^8.2.0",
        "enzyme": "^3.10.0",
        "express": "^4.17.1",
        "express-http-proxy": "^1.6.0",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jquery": "^3.4.1",
        "json-loader": "^0.5.7",
        "mini-css-extract-plugin": "^0.5.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "passport": "^0.4.0",
        "passport-google-oauth": "^2.0.0",
        "pokedex-promise-v2": "^3.2.0",
        "react": "^16.9.0",
        "react-bootstrap": "^1.0.0-beta.12",
        "react-dom": "^16.9.0",
        "react-google-login": "^5.0.5",
        "react-hot-loader": "^4.12.14",
        "react-inlinesvg": "^0.8.4",
        "react-redux": "^6.0.1",
        "react-tabs": "^3.0.0",
        "react-transition-group": "^4.3.0",
        "redux": "^4.0.4",
        "redux-mock-store": "^1.5.3",
        "redux-thunk": "^2.3.0",
        "style-loader": "^0.23.1",
        "svg-inline-loader": "^0.8.0",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.0",
        "webpack-cli": "^3.3.9",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-dev-server": "^3.8.1",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-node-externals": "^1.7.2"
    }
}
